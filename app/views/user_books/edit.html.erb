<%= render partial: 'books/show', locals: {book: @book} %>
<% if !@user_book.valid? %>
	<% @user_book.errors.messages.each do |key, value|  %>
		<strong><%= "#{key}, #{value.first}" %></strong><br>
	<% end %>
<% end %>

<%= form_for(@user_book, url: editUserBook_path(@user_book)) do |f| %>
	<%= f.label :page, "New Page" %>
	<%= f.text_field :page %><br>
	<%= f.submit "Update Bookmark"%>
<% end %>
